<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!--
    import this file in a .csproj to handle signing correctly
  -->
  <PropertyGroup>
    <SignAssembly>true</SignAssembly>
    <KeyDirectory>keys</KeyDirectory>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyOriginatorKeyFile>$(KeyDirectory)\internalkey.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
</Project>
